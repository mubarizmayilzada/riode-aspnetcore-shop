@section addjs{
    <script>
        $(document).ready(function () {
            $('.toolbox-layout a[href^="#product-"]').click(function (e) {
                e.preventDefault();

                $(e.currentTarget).parent().find('a').removeClass('active');
                $(e.currentTarget).addClass('active');

                const isListLayout = $(e.currentTarget).attr('href') == '#product-list';
                const data = $(e.currentTarget).data();

                const container = $('#productContainer');
                $(container).attr('class', data.containerClass);

                $(container).find('.product').each(function (index, item) {
                    const actionContainer = $(item).find('.product-action')

                    if (isListLayout == true) {

                        $(item).find('.product-details').append(actionContainer);

                    }

                    else {

                        $(item).find('.product-media').append(actionContainer);

                    }
                })
            })



        })

        function setFilter() {

            if ($.setFilterProcessId != undefined) {
                clearTimeout($.setFilterProcessId);
            };


            $.setFilterProcessId = setTimeout(function () {
                const filterObj = {};
                $('ul[data-entity-name]').each(function (index, item) {
                    const entityPropertyName = $(item).data('entity-name');

                    filterObj[entityPropertyName] = [];


                    $(item).find('li.active').each(function (i, v) {
                        const entityId = $(v).data('entity-id');
                        filterObj[entityPropertyName].push(entityId);

                    })
                });

                console.log(filterObj)
/*                const FilterObjJson = JSON.stringify(filterObj);*/
                $.ajax({
                    url: '@Url.Action("Filter")',
                    type: 'POST',

                    data: filterObj,
                    contentType: 'application/x-www-form-urlencoded',

                    //data: FilterObjJson,
                    //contentType: 'application/json',

                    //dataType: 'json',
                    dataType: 'html',
                    beforeSend: function () {
                        console.log('sorgudan evvel');
                    },
                    success: function (response) {
                        console.log(response);

                        $('#productContainer').html(response);

                        const el = $('a[href="#product-list"].active');
                        if (el != undefined) {
                            $(el)[0].click('');
                        }
                    },
                    error: function (response) {
                        console.log(response);
                    }
                })
                    .always(function () {
                        console.log('sorgudan sonra');
                    })
            },2000);
        }
    </script>
}